n  margin: auto;\n  left: ${(props) => props.direction === \"left\" && \"10px\"};\n  right: ${(props) => props.direction === \"right\" && \"10px\"};\n  cursor: pointer;\n  opacity: 0.5;\n  z-index: 2;\n`;\n\n\n\nconst Slider = () => {\n  const [slideIndex, setSlideIndex] = useState(0);\n\n  const handleClick = (direction) => {\n    if (direction === \"left\") {\n      setSlideIndex(slideIndex > 0 ? slideIndex - 1 : 2);\n    } else {\n      setSlideIndex(slideIndex < 2 ? slideIndex + 1 : 0);\n    }\n  };\n\n  return (\n    <Container>\n      <Arrow direction=\"left\" onClick={() => handleClick(\"left\")}>\n        <ArrowLeftOutlined />\n      </Arrow>\n      <Wrapper slideIndex={slideIndex}>\n        {sliderItems.map((item) => (\n          <Slide bg={item.bg} key={item.id}>\n            <ImgContainer>\n              <Image src={item.img} />\n            </ImgContainer>\n            <InfoContainer>\n              <Title to={item.path}>{item.title}</Title>\n              <Description>{item.description}</Description>\n              \n            </InfoContainer>\n          </Slide>\n        ))}\n      </Wrapper>\n      <Arrow direction=\"right\" onClick={() => handleClick(\"right\")}>\n        <ArrowRightOutlined />\n      </Arrow>\n    </Container>\n  );\n};\n\nexport default Slider;\n","/Users/manojgowdavenkatachalamurthy/Desktop/projects/IWS_project/systune/src/data.js",[],[],"/Users/manojgowdavenkatachalamurthy/Desktop/projects/IWS_project/systune/src/pages/GTA.js",[],[],"/Users/manojgowdavenkatachalamurthy/Desktop/projects/IWS_project/systune/src/pages/COD.js",[],[],"/Users/manojgowdavenkatachalamurthy/Desktop/projects/IWS_project/systune/src/pages/Fortnite.js",[],[],"/Users/manojgowdavenkatachalamurthy/Desktop/projects/IWS_project/Term_Project-5130f2022/src/index.js",[],[],[],"/Users/manojgowdavenkatachalamurthy/Desktop/projects/IWS_project/Term_Project-5130f2022/src/App.js",[],[],"/Users/manojgowdavenkatachalamurthy/Desktop/projects/IWS_project/Term_Project-5130f